{
  "name": "aws_VOD_BE",
  "version": "0.0.1",
  "description": "aws_VOD_BE",
  "author": {
    "name": "Nick Zheng",
    "email": "nickzhengyiming@gmail.com"
  },
  "private": false,
  "scripts": {
    "start": "NODE_ENV=development nest start --watch",
    "build": "nest build && cp -r config/* dist/config/ && cp Procfile dist/ && cp package.json dist/ && cp -r .platform dist && cd dist && zip ../myapp.zip -r * .[^.]*",
    "release": "eb deploy",
    "docker-compose": "docker-compose -f ./docker-compose.yml up -d",
    "typeorm": "ts-node -r ts-node/register ./node_modules/typeorm/cli.js",
    "prebuild": "rimraf dist",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=development jest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "pre-push": "npm run format && npm run lint && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "aws-sdk": "^2.1048.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^2.10.1",
    "cache-manager-redis-store": "^1.5.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config": "^3.2.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "ts-node": "^8.4.1",
    "typeorm": "^0.2.20",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/jest": "^24.0.18",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.8",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-alloy": "^0.2.1",
    "tslint-config-prettier": "^1.18.0"
  }
}
